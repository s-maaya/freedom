<!DOCTYPE html>
<html>
  <head>
    <title>Portfolio</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <script src="https://kit.fontawesome.com/150fad54ca.js" crossorigin="anonymous"></script>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <header class="header-line">
  <div class="continer">
        <p class="logo-icon">
          logo_enduser
        </p>
          <%= link_to new_room_path do %>
            <p class="header-icon"><span class="fas fa-folder-plus fa-fx-3"></span></p>
          <%end%>
          <%= link_to new_user_registration_path do %>
            <p class="header-icon"><span class="fas fa-user-plus fa-fx-3"></span></p>
          <%end%>
          <%= link_to rooms_path do %>
            <p class="header-icon"><span class="fas fa-th-large fa-fx-3"></span></p>
          <%end%>
          <%= link_to new_user_registration_path do %>
            <p class="header-icon"><span class="fas fa-sign-in-alt fa-fx-3"></span></p>
          <%end%>
  </div>
  </header>
  <%= yield %>

  </body>
  </html>

