<!DOCTYPE html>
<html>
  <head>
    <title>Portfolio</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <script src="https://kit.fontawesome.com/150fad54ca.js" crossorigin="anonymous"></script>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
  <header class="header-line">
  <div class="continer">
        <p class="logo-icon">
          logo_adminuser
        </p>
          <%= link_to new_admin_admin_user_session_path do %>
            <p class="header-icon">新規登録<span class="fas fa-user-plus fa-fx-3"></span></p>
          <%end%>
          <%= link_to admin_rooms_path do %>
            <p class="header-icon">投稿一覧<span class="fas fa-th-large fa-fx-3"></span></p>
          <%end%>
          <%= link_to destroy_admin_admin_user_session_path,  method: :delete do %>
            <p class="header-icon">ログアウト<span class="fas fa-sign-out-alt fa-fx-3"></span></p>
          <%end%>
  </div>
  </header>
  <%= yield %>

  </body>
  </html>

